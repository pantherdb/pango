{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "paths": {
      "@panther": [
        "@panther/"
      ],
      "@panther.common": [
        "@panther.common"
      ],
    },
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
