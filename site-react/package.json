{
  "name": "pango-site",
  "private": false,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite --mode development --host 0.0.0.0 --port 4208",
    "start:staging": "vite --mode staging --host 0.0.0.0 --port 4208",
    "build": "tsc && vite build",
    "build:staging": "rm -rf dist && tsc && vite build --mode staging",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.10.6",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/system": "^5.15.20",
    "@mui/utils": "^5.15.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@rtk-query/graphql-request-base-query": "^2.3.1",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.7.2",
    "framer-motion": "^11.11.17",
    "graphql-request": "^7.0.1",
    "panther-overrep-form": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.0",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.23.1",
    "vite-plugin-checker": "^0.8.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jsdom": "^23.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.4"
  }
}